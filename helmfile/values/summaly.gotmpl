ingress:
  enabled: true
  className: "traefik-external"
  annotations:
    cert-manager.io/cluster-issuer: letsencrypt
    external-dns.alpha.kubernetes.io/target: conohav2-1.pstr.space
  hosts:
    - host: summaly.pstr.space
      paths:
        - path: /
          pathType: Prefix
  tls:
    - hosts:
        - summaly.pstr.space
      secretName: summaly.pstr.space-dns01